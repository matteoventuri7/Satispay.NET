<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Satispay.NET</PackageId>
    <AssemblyName>SatispayGBusiness</AssemblyName>
    <RootNamespace>SatispayGBusiness</RootNamespace>
    <LangVersion>preview</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.10</Version>
    <Authors>Giovanni Quarella</Authors>
    <PackageTags>satispay</PackageTags>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon></PackageIcon>
    <Description>Satispay C# API client based on https://developers.satispay.com/reference</Description>
    <RepositoryUrl>https://github.com/GioviQ/Satispay.NET</RepositoryUrl>
    <PackageProjectUrl>https://github.com/GioviQ/Satispay.NET</PackageProjectUrl>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BouncyCastle.Cryptography" Version="2.2.1" />
    <PackageReference Include="System.Net.Http.Json" Version="7.0.1" />
  </ItemGroup>

</Project>
